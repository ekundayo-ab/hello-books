{
    "parser":"babel-eslint",
    "extends": "airbnb",
    "rules": {
      "react/jsx-filename-extension": [0, { "extensions": [".js", ".jsx"] }],
      "react/forbid-prop-types": [0, { "forbid": ["object"] }],
      "react/prefer-stateless-function": "off",
      "import/no-unresolved": "off",
      "jsx-a11y/href-no-hash": "off",
      "import/extensions": "off",
      "react/sort-comp": "off"
  },
  "globals": {
    "browser": true,
    "node": true,
    "window": true,
    "document": true,
    "Materialize": true
  },
  "env": {
    "mocha": true,
    "jquery": true,
    "commonjs": true,
    "es6": true,
    "browser": true
  }
}