{
    "parser":"babel-eslint",
    "extends": "airbnb",
    "rules": {
      "react/jsx-filename-extension": [0, { "extensions": [".js", ".jsx"] }],
      "react/forbid-prop-types": [0, { "forbid": ["object"] }],
      "import/no-unresolved": "off",
      "jsx-a11y/href-no-hash": "off",
      "jsx-a11y/anchor-is-valid": ["warn", { "aspects": ["invalidHref"] }]
  },
  "globals": {
    "browser": true,
    "node": true,
    "window": true,
    "document": true
  },
  "env": {
    "mocha": true,
    "jquery": true,
    "commonjs": true,
    "es6": true
  }
}