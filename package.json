{
  "name": "hello-books",
  "version": "1.0.0",
  "description": "Hello-Books is a simple application that helps manage a library and its processes like stocking, tracking and renting of books.",
  "main": "app.js",
  "jest": {
    "verbose": true,
    "roots": ["client/tests/"]
  },
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "start": "babel-node app.js",
    "start:dev": "babel-watch app.js",
    "build": "webpack --config webpack.config.prod.js -p --progress --colors",
    "build:dev": "webpack-dev-server --open --config webpack.config.dev.js",
    "test": "cross-env NODE_ENV=test mocha --timeout 5000 --compilers js:babel-register server/tests/**.js",
    "test-client": "jest",
    "test:db": "sequelize db:migrate",
    "coverage": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text --reporter=lcovonly mocha --compilers js:babel-register server/tests/**.js",
    "db:migrate": "cross-env DATABASE_URL=$DATABASE_URL sequelize db:migrate",
    "db:seed": "DATABASE_URL=$DATABASE_URL sequelize db:seed:all",
    "heroku-postbuild": "NODE_ENV=production && NODE_ENV=production sequelize db:migrate:undo:all && NODE_ENV=production sequelize db:migrate && NODE_ENV=production npm run db:seed && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ekundayo-ab/hello-books.git"
  },
  "keywords": [
    "books",
    "library",
    "borrow"
  ],
  "author": "Ekundayo Abiona",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ekundayo-ab/hello-books/issues"
  },
  "nyc": {
    "exclude": [
      "server/tests"
    ]
  },
  "homepage": "https://github.com/ekundayo-ab/hello-books#readme",
  "dependencies": {
    "axios": "^0.16.2",
    "babel-cli": "^6.26.0",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-watch": "^2.0.7",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.17.2",
    "classnames": "^2.2.5",
    "css-loader": "0.28.4",
    "dotenv": "4.0.0",
    "eslint": "^4.5.0",
    "express": "^4.15.4",
    "file-loader": "0.11.2",
    "jquery": "^3.2.1",
    "jsonwebtoken": "^8.0.1",
    "lodash": "^4.17.4",
    "materialize-css": "^0.100.2",
    "moment": "^2.19.1",
    "morgan": "^1.8.2",
    "node-sass": "^4.5.3",
    "pg": "^7.2.0",
    "pg-hstore": "^2.3.2",
    "promise": "8.0.1",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-dropzone": "^4.1.3",
    "react-google-login": "^3.0.1",
    "react-materialize": "^1.0.14",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-thunk": "^2.2.0",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "sequelize": "^4.8.0",
    "sequelize-cli": "^2.8.0",
    "shortid": "^2.2.8",
    "style-loader": "0.18.2",
    "swagger-jsdoc": "^1.9.7",
    "sweetalert": "^2.0.5",
    "url-loader": "0.5.9",
    "validator": "^8.1.0",
    "webpack": "3.5.1",
    "webpack-dev-server": "2.7.1"
  },
  "devDependencies": {
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "codeclimate-test-reporter": "^0.5.0",
    "coveralls": "^2.13.1",
    "cross-env": "^5.0.5",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.3.0",
    "faker": "^4.1.0",
    "jest": "^21.2.1",
    "mocha": "^3.5.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^11.1.0",
    "sass-lint": "^1.11.1",
    "should": "^11.2.1",
    "supertest": "^3.0.0"
  }
}
